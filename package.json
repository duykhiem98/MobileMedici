{
    "name": "medici",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "init-project": "react-native eject && npm run pod-install",
        "start": "react-native start",
        "android-dev": "react-native run-android --no-jetifier --variant=devDebug --appId 'demo.dev'",
        "android-staging": "cd android && ./gradlew clean && cd .. && react-native run-android --no-jetifier --variant=stagingDebug --appId 'demo.stg'",
        "android-product": "cd android && ./gradlew clean && cd .. && react-native run-android --no-jetifier --variant=productDebug --appId 'demo'",
        "android-apk-staging": "export ENVFILE=.env.staging && cd android && ./gradlew assembleStagingRelease && cd ..",
        "android-apk-dev": "export ENVFILE=.env.development && cd android && ./gradlew assembleDevRelease && cd ..",
        "android-apk-product": "export ENVFILE=.env.production && cd android && ./gradlew assembleProductRelease && cd ..",
        "android-bundle-product": "export ENVFILE=.env.production && cd android && ./gradlew bundleProductRelease && cd ..",
        "android-bundle-staging": "export ENVFILE=.env.staging && cd android && ./gradlew bundleStagingRelease && cd ..",
        "ios": "ENVFILE=.env.development react-native run-ios",
        "push-ios-dev": "appcenter codepush release-react -a teracom/app-name -d Development -t 1.0.0 --extra-bundler-option=\"--reset-cache\"",
        "push-ios-staging": "appcenter codepush release-react -a teracom/app-name -d Staging -t 1.0.0 --extra-bundler-option=\"--reset-cache\"",
        "push-ios-production": "appcenter codepush release-react -a teracom/app-name -d Production -t 1.0.0 --extra-bundler-option=\"--reset-cache\"",
        "push-android-dev": "appcenter codepush release-react -a teracom/app-name -d Development -t 1.0.0",
        "push-android-staging": "appcenter codepush release-react -a teracom/app-name -d Staging -t 1.0.0",
        "push-android-production": "appcenter codepush release-react -a teracom/app-name -d Production -t 1.0.0",
        "push-develop": "yarn push-ios-dev && yarn push-android-dev",
        "push-staging": "yarn push-ios-staging && yarn push-android-staging",
        "push-production": "yarn push-ios-production && yarn push-android-production",
        "test": "jest",
        "pod-install": "cd ios && pod install && cd ..",
        "postinstall": "cd scripts && sh ./fix-lib.sh && cd .. && npx jetifier",
        "assets-link": "npx react-native link",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint:ts": "tslint 'src/**/*.{ts,tsx,js}'",
        "lint-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
        "devtools": "react-devtools",
        "link": "react-native link"
    },
    "dependencies": {
        "@hookform/resolvers": "^2.8.8",
        "@react-native-async-storage/async-storage": "^1.17.2",
        "@react-native-community/cameraroll": "^4.1.2",
        "@react-native-community/masked-view": "^0.1.11",
        "@react-native-firebase/app": "^14.7.0",
        "@react-native-firebase/auth": "^14.7.0",
        "@react-native-google-signin/google-signin": "^7.2.2",
        "@react-navigation/bottom-tabs": "^6.2.0",
        "@react-navigation/native": "^6.0.8",
        "@react-navigation/native-stack": "^6.5.0",
        "@react-navigation/stack": "^6.1.1",
        "@redux-saga/core": "^1.1.3",
        "@reduxjs/toolkit": "^1.8.0",
        "@types/react": "^17",
        "dayjs": "^1.11.0",
        "debounce": "^1.2.1",
        "i18next": "^21.6.14",
        "install": "^0.13.0",
        "moment": "^2.29.1",
        "npm": "^8.5.4",
        "react": "17.0.2",
        "react-hook-form": "^7.28.0",
        "react-i18next": "^11.15.6",
        "react-native": "0.67.3",
        "react-native-calendar-picker": "^6.1.5",
        "react-native-config": "^1.4.5",
        "react-native-date-picker": "^4.2.0",
        "react-native-fbsdk-next": "^7.3.3",
        "react-native-gesture-handler": "^2.3.1",
        "react-native-iphone-x-helper": "^1.3.1",
        "react-native-keyboard-aware-scroll-view": "^0.9.5",
        "react-native-linear-gradient": "^2.5.6",
        "react-native-localize": "^2.2.0",
        "react-native-media-console": "^1.2.7",
        "react-native-media-controls": "^2.3.0",
        "react-native-modal": "^13.0.1",
        "react-native-orientation-locker": "^1.4.0",
        "react-native-parallax-header": "^1.1.4",
        "react-native-portalize": "^1.0.7",
        "react-native-safe-area-context": "^4.1.2",
        "react-native-screens": "^3.13.1",
        "react-native-size-matters": "^0.4.0",
        "react-native-slider": "^0.11.0",
        "react-native-svg": "^12.3.0",
        "react-native-svg-transformer": "^1.0.0",
        "react-native-video": "^5.2.0",
        "react-number-format": "^4.9.1",
        "react-redux": "^7.2.6",
        "redux-persist": "^6.0.0",
        "redux-saga": "^1.1.3",
        "rn-fetch-blob": "^0.12.0",
        "use-debounce": "^7.0.1",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@babel/runtime": "^7.12.5",
        "@react-native-community/eslint-config": "^2.0.0",
        "@types/jest": "^26.0.23",
        "@types/react-native": "^0.66.15",
        "@types/react-native-calendar-picker": "^7.0.2",
        "@types/react-native-video": "^5.0.12",
        "@types/react-test-renderer": "^17.0.1",
        "@typescript-eslint/eslint-plugin": "^5.7.0",
        "@typescript-eslint/parser": "^5.7.0",
        "babel-jest": "^26.6.3",
        "babel-plugin-module-resolver": "^4.1.0",
        "eslint": "^7.14.0",
        "jest": "^26.6.3",
        "metro-react-native-babel-preset": "^0.66.2",
        "react-test-renderer": "17.0.2",
        "typescript": "^4.4.4"
    },
    "resolutions": {
        "@types/react": "^17"
    },
    "jest": {
        "preset": "react-native",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    }
}
